! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:  rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault


! xscreensaver ---------------------------------------------------------------

!font settings
xscreensaver.Dialog.headingFont:        -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
xscreensaver.Dialog.bodyFont:           -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
xscreensaver.Dialog.buttonFont:         -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
xscreensaver.Dialog.dateFont:           -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
xscreensaver.passwd.passwdFont:         --windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #eeeeee
xscreensaver.Dialog.background:         #1c1c1c
xscreensaver.Dialog.topShadowColor:     #1c1c1e
xscreensaver.Dialog.bottomShadowColor:  #1c1c1e
xscreensaver.Dialog.Button.foreground:  #eeeeee
xscreensaver.Dialog.Button.background:  #444
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #eeeeee
xscreensaver.Dialog.text.background:    #444
xscreensaver.Dialog.internalBorderWidth:0
xscreensaver.Dialog.borderWidth:        1
xscreensaver.Dialog.shadowThickness:    2
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #0084ff
xscreensaver.passwd.thermometer.background:  #1c1c1c
xscreensaver.passwd.thermometer.width:       8
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y

XTerm*termName: xterm-256color

URxvt*termName: rxvt-256color
#URxvt*title: urxvt
#URxvt*transparent: true
#URxvt*shading: 50
urxvt*depth: 32
urxvt*background: [80]#000000
#URxvt*tintColor: #000000
#URxvt*blurRadius: 15
URxvt*saveLines: 12000
URxvt*cursorBlink: false
#URxvt*cursorUnderline: true
URxvt*underlineColor: #93d44f
URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt*scrollstyle: plain
URxvt*scrollColor: #0a0a0a
URxvt*borderLess:           false
URxvt*inheritPixmap:	    false
URxvt.perl-lib:             /usr/lib/urxvt/perl
URxvt.perl-ext-common:      default,matcher,url-select,keyboard-select
URxvt.urlLauncher: /usr/bin/firefox
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.matcher.button: 1 
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

